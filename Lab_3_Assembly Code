	    AREA labtask, CODE, READONLY
		ENTRY
		EXPORT main
		EXPORT task1
		EXPORT task2
		EXPORT task3
		EXPORT task4
		EXPORT task5
X EQU 9
Y EQU 8
Z EQU 5
main
task1
	MOV r1, #2
	MOV r2, #4
	MOV r3, #5
	ADD r0,r1,r2
	ADD r0, r3

task2
	MOV r1, #15
	MOV r2, #5
	SUB r0,r1,r2

task3
	MOV r1, #12
	MOV r2, #4
	MOV r3, #5
	SUB r0,r1,r2
	SUB r0, r1, r0

task4
	MOV r1, #3
	MOV r2, #5
	MUL r0,r1,r2

task5
	MOV r4, #X
	MOV r3, #Y
	MOV r2, #Z
	ADD r0,r3,r4
	ADD r0, r2
Stop  B Stop
     END
